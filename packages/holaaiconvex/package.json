{
  "name": "@holaai/convex",
  "version": "1.0.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "dev": "convex dev",
    "dev:once": "convex dev --once",
    "dev:prod": "dotenv -e .env.production -- bash -c 'echo \"=> Target: $CONVEX_URL (PROD)\" && convex dev'",
    "dev:prod:once": "dotenv -e .env.production -- bash -c 'echo \"=> Target: $CONVEX_URL (PROD)\" && convex dev --once'",
    "convex:deploy": "dotenv -e .env.local -- bash -c 'echo \"=> Deploying to: $CONVEX_URL (DEV)\" && convex deploy'",
    "convex:deploy:prod": "dotenv -e .env.production -- bash -c 'echo \"=> Deploying to: $CONVEX_URL (PROD)\" && convex deploy'",
    "deploy:prod": "dotenv -e .env.production -- bash -c 'echo \"=> Deploying to: $CONVEX_URL (PROD)\" && convex deploy'",
    "deploy:prod:yes": "dotenv -e .env.production -- bash -c 'echo \"=> Deploying to: $CONVEX_URL (PROD)\" && convex deploy --yes'",
    "typecheck": "tsc --noEmit",
    "seed:prod": "./scripts/seed-prod.sh",
    "seed:dev": "./scripts/seed-prod.sh --dev",
    "env:sync": "./scripts/sync-env.sh",
    "env:sync:prod": "./scripts/sync-env.sh --prod",
    "env:sync:all": "./scripts/sync-env.sh --all",
    "env:sync:all:prod": "./scripts/sync-env.sh --all --prod",
    "env:list": "./scripts/sync-env.sh --list",
    "env:list:prod": "./scripts/sync-env.sh --list --prod",
    "env:check": "./scripts/check-env-parity.sh",
    "env:check:verbose": "./scripts/check-env-parity.sh --verbose",
    "env:update": "./scripts/update-env.sh",
    "env:update:dev": "./scripts/update-env.sh --target dev --file",
    "env:update:prod": "./scripts/update-env.sh --target prod --file"
  },
  "exports": {
    ".": "./index.ts",
    "./_generated/api": "./convex/_generated/api.js",
    "./_generated/dataModel": "./convex/_generated/dataModel.js",
    "./convex/_generated/api": "./convex/_generated/api.js",
    "./convex/_generated/dataModel": "./convex/_generated/dataModel.js",
    "./schema": "./convex/schema.ts"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^2.0.22",
    "@ai-sdk/openai": "^2.0.88",
    "@clerk/backend": "^1.21.0",
    "@convex-dev/agent": "^0.3.2",
    "ai": "^5.0.115",
    "convex": "^1.29.0",
    "croner": "^10.0.1",
    "jsonwebtoken": "^9.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.0.0",
    "dotenv-cli": "^11.0.0",
    "typescript": "~5.9.2"
  }
}
