{
  "name": "taurireact-macapp",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mobile": "vite --host 0.0.0.0 --port 1420",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:preview": "bash ./scripts/e2e-preview-run.sh",
    "tauri": "tauri",
    "tauri:staging": "TAURI_BUILD_MODE=staging tauri build -c src-tauri/tauri.staging.conf.json",
    "tauri:production": "TAURI_BUILD_MODE=production VITE_CLERK_PUBLISHABLE_KEY=pk_live_Y2xlcmsucmpsYWJzLmRldiQ tauri build -c src-tauri/tauri.production.conf.json",
    "lint:rust": "cd src-tauri && cargo clippy --all -- -D warnings",
    "fmt:rust": "cd src-tauri && cargo fmt --all",
    "env:check": "./scripts/check-vercel-env-parity.sh",
    "env:check:verbose": "./scripts/check-vercel-env-parity.sh --verbose",
    "env:update": "./scripts/update-vercel-env.sh",
    "env:update:preview": "./scripts/update-vercel-env.sh --target preview --file",
    "env:update:staging": "./scripts/update-vercel-env.sh --target staging --file",
    "env:update:production": "./scripts/update-vercel-env.sh --target production --file",
    "cap:sync": "npx cap sync",
    "cap:sync:prod": "CAP_ENV=prod npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:ios:prod": "CAP_ENV=prod npx cap open ios",
    "cap:open:android": "npx cap open android",
    "build:mobile": "vite build --mode development && npx cap sync",
    "build:mobile:production": "vite build --mode production && CAP_ENV=prod npx cap sync",
    "cap:ios:dev": "vite build --mode development && npx cap sync && npx cap open ios",
    "cap:ios:prod": "vite build --mode production && CAP_ENV=prod npx cap sync && CAP_ENV=prod npx cap open ios",
    "cap:ios:prod:build": "bash ./scripts/capacitor-ios-prod-build.sh",
    "cap:ios:prod:install": "bash ./scripts/capacitor-ios-prod-install.sh"
  },
