{
  "name": "taurireact-macapp",
  "private": true,
  "version": "1.0.23",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:production": "vite --mode production",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:preview": "bash ./scripts/e2e-preview-run.sh",
    "tauri": "tauri",
    "tauri:dev:production": "VITE_CLERK_PUBLISHABLE_KEY=pk_live_Y2xlcmsucmpsYWJzLmRldiQ tauri dev -c src-tauri/tauri.dev-production.conf.json",
    "tauri:production": "TAURI_BUILD_MODE=production VITE_CLERK_PUBLISHABLE_KEY=pk_live_Y2xlcmsucmpsYWJzLmRldiQ tauri build -c src-tauri/tauri.production.conf.json",
    "lint:rust": "cd src-tauri && cargo clippy --all -- -D warnings",
    "fmt:rust": "cd src-tauri && cargo fmt --all",
    "env:check": "./scripts/check-vercel-env-parity.sh",
    "env:check:verbose": "./scripts/check-vercel-env-parity.sh --verbose",
    "env:update": "./scripts/update-vercel-env.sh",
    "env:update:preview": "./scripts/update-vercel-env.sh --target preview --file",
    "env:update:production": "./scripts/update-vercel-env.sh --target production --file",
    "cap:ios:add": "npx cap add ios",
    "cap:ios:dev:sync": "pnpm build:dev && cp capacitor.config.dev.ts capacitor.config.ts && npx cap sync ios && node ./scripts/capacitor-ios-postsync.mjs",
    "cap:ios:dev:open": "cp capacitor.config.dev.ts capacitor.config.ts && node ./scripts/capacitor-ios-postsync.mjs && npx cap open ios",
    "cap:ios:prod:sync": "pnpm build:production && cp capacitor.config.prod.ts capacitor.config.ts && npx cap sync ios --deployment && node ./scripts/capacitor-ios-postsync.mjs",
    "cap:ios:prod:open": "cp capacitor.config.prod.ts capacitor.config.ts && node ./scripts/capacitor-ios-postsync.mjs && npx cap open ios",
    "cap:ota:build": "pnpm build:production && cd dist && zip -r ../ota-update.zip . && cd .. && echo 'OTA bundle created: ota-update.zip'",
    "cap:ota:build:version": "pnpm build:production && node ./scripts/create-ota-bundle.mjs",
    "ota:bundle:dev": "pnpm build:dev && node ./scripts/create-ota-bundle.mjs",
    "ota:bundle:prod": "pnpm build:production && node ./scripts/create-ota-bundle.mjs",
    "ota:upload:dev": "node ./scripts/upload-ota-to-convex.mjs dev",
    "ota:upload:prod": "node ./scripts/upload-ota-to-convex.mjs prod",
    "ota:release:dev": "node ./scripts/ota-release.mjs dev",
    "ota:release:prod": "node ./scripts/ota-release.mjs prod"
  },
