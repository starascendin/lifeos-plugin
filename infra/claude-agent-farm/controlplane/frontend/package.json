{
  "name": "controlplane-ui",
  "version": "1.0.39",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "cap:sync": "npm run build && npx cap sync && ./scripts/patch-ios.sh",
    "cap:ios": "npm run build && npx cap sync ios && ./scripts/patch-ios.sh && npx cap open ios",
    "cap:ios:dev": "npx cap run ios --livereload --external"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@playwright/test": "^1.58.0",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "bits-ui": "^0.22.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.563.0",
    "mode-watcher": "^0.4.1",
    "postcss": "^8.4.32",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@capacitor/app": "^6.0.3",
    "@capacitor/core": "^6.2.1",
    "@capacitor/ios": "^6.2.1",
    "@capgo/capacitor-updater": "^6.0.0",
    "marked": "^11.1.0"
  },
  "type": "module"
}
